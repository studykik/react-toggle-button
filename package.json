{
  "name": "react-boilerplate",
  "version": "3.1.0",
  "description": "A highly scalable, offline-first foundation with the best DX and a focus on performance and best practices",
  "repository": {
    "type": "git",
    "url": "git://github.com/mxstbr/react-boilerplate.git"
  },
  "engines": {
    "node": ">= 6.2.0"
  },
  "author": "Max Stoiber",
  "license": "MIT",
  "scripts": {
    "analyze:clean": "rimraf stats.json",
    "preanalyze": "npm run analyze:clean",
    "analyze": "node ./internals/scripts/analyze.js",
    "extract-intl": "babel-node --presets es2015,stage-0 -- ./internals/scripts/extract-intl.js",
    "npmcheckversion": "node ./internals/scripts/npmcheckversion.js",
    "preinstall": "npm run npmcheckversion",
    "install": "node install.js",
    "postinstall": "npm run build:dll",
    "prebuild": "npm run build:clean",
    "build": "cross-env NODE_ENV=production webpack --config internals/webpack/webpack.prod.babel.js --color -p",
    "build:clean": "npm run test:clean && rimraf ./build",
    "build:dll": "node ./internals/scripts/dependencies.js",
    "start": "cross-env NODE_ENV=development node server",
    "start:tunnel": "cross-env NODE_ENV=development ENABLE_TUNNEL=true node server",
    "start:production": "npm run build && npm run start:prod",
    "start:prod": "cross-env NODE_ENV=production node server",
    "pagespeed": "node ./internals/scripts/pagespeed.js",
    "presetup": "npm i chalk shelljs",
    "setup": "node ./internals/scripts/setup.js",
    "postsetup": "npm run build:dll",
    "clean": "shjs ./internals/scripts/clean.js",
    "clean:all": "npm run analyze:clean && npm run test:clean && npm run build:clean",
    "generate": "plop --plopfile internals/generators/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:css": "stylelint ./app/**/*.less",
    "pretest": "npm run test:clean && npm run lint",
    "test:clean": "rimraf ./coverage",
    "test#disabled": "cross-env NODE_ENV=test karma start internals/testing/karma.conf.js --single-run",
    "test:watch": "npm run test -- --auto-watch --no-single-run",
    "test:firefox": "npm run test -- --browsers Firefox",
    "test:safari": "npm run test -- --browsers Safari",
    "test:ie": "npm run test -- --browsers IE",
    "coveralls": "cat ./coverage/lcov/lcov.info | coveralls"
  },
  "dllPlugin": {
    "path": "node_modules/react-boilerplate-dlls",
    "exclude": [
      "chalk",
      "compression",
      "cross-env",
      "dotenv",
      "diffie-hellman",
      "elliptic",
      "express",
      "fs",
      "ip",
      "html-to-pdf",
      "minimist",
      "moment-timezone",
      "parse-asn1",
      "react-icons",
      "sanitize.css"
    ],
    "include": [
      "core-js",
      "lodash",
      "eventsource-polyfill"
    ]
  },
  "dependencies": {
    "babel-cli": "6.11.4",
    "babel-core": "6.13.2",
    "babel-eslint": "6.1.2",
    "babel-loader": "6.2.4",
    "babel-plugin-react-intl": "2.1.3",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-constant-elements": "6.9.1",
    "babel-plugin-transform-react-inline-elements": "6.8.0",
    "babel-plugin-transform-react-remove-prop-types": "0.2.9",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "6.13.2",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-0": "6.5.0",
    "blueimp-canvas-to-blob": "^3.6.0",
    "chai": "3.5.0",
    "chai-enzyme": "0.5.0",
    "chalk": "1.1.3",
    "cheerio": "0.20.0",
    "classnames": "^2.2.5",
    "compression": "1.6.2",
    "coveralls": "2.11.12",
    "cross-env": "2.0.0",
    "css-loader": "0.23.1",
    "dotenv": "^2.0.0",
    "enzyme": "2.4.1",
    "eslint": "3.3.0",
    "eslint-config-airbnb": "10.0.1",
    "eslint-import-resolver-webpack": "0.5.1",
    "eslint-plugin-import": "1.13.0",
    "eslint-plugin-jsx-a11y": "2.1.0",
    "eslint-plugin-react": "6.0.0",
    "expect": "1.20.2",
    "expect-jsx": "2.6.0",
    "exports-loader": "0.6.3",
    "express": "4.14.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "fontfaceobserver": "2.0.1",
    "git-validate": "^2.2.2",
    "google-libphonenumber": "^2.0.3",
    "history": "3.0.0",
    "html-loader": "0.4.3",
    "html-webpack-plugin": "2.22.0",
    "image-webpack-loader": "2.0.0",
    "imports-loader": "0.6.5",
    "intl": "1.2.4",
    "invariant": "2.2.1",
    "ip": "1.1.3",
    "isparta": "4.0.0",
    "isparta-loader": "2.0.0",
    "json-loader": "0.5.4",
    "karma": "1.2.0",
    "karma-chrome-launcher": "1.0.1",
    "karma-coverage": "1.1.1",
    "karma-firefox-launcher": "1.0.0",
    "karma-ie-launcher": "1.0.0",
    "karma-junit-reporter": "0.2.2",
    "karma-mocha": "1.1.1",
    "karma-mocha-reporter": "2.1.0",
    "karma-safari-launcher": "1.0.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "1.8.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "lodash": "4.15.0",
    "mocha": "3.0.2",
    "moment": "^2.15.1",
    "moment-timezone": "^0.5.6",
    "ngrok": "2.2.2",
    "null-loader": "0.1.1",
    "offline-plugin": "3.4.2",
    "postcss-loader": "0.9.1",
    "react": "15.3.2",
    "react-addons-update": "15.3.0",
    "react-avatar-editor": "^6.0.0",
    "react-big-calendar": "git+https://github.com/studykik/kik-react-big-calendar.git#64151390ded600826e88696cf7fe2d2b338a4bf7",
    "react-bootstrap": "^0.30.3",
    "react-click-outside": "^2.1.0",
    "react-date-range": "^0.2.4",
    "react-dnd": "^2.1.4",
    "react-dnd-html5-backend": "^2.1.2",
    "react-dom": "15.3.2",
    "react-helmet": "3.1.0",
    "react-icons": "^2.2.1",
    "react-infinite-scroller": "^1.0.1",
    "react-intl": "2.1.3",
    "react-redux": "4.4.5",
    "react-redux-toastr": "3.6.4",
    "react-router": "2.6.1",
    "react-router-redux": "4.0.5",
    "react-router-scroll": "0.2.1",
    "react-scroll": "^1.4.4",
    "react-select": "^1.0.0-rc.2",
    "react-sound": "^0.5.2",
    "react-sticky": "^5.0.5",
    "react-super-select": "^0.5.7",
    "react-toggle-button": "^2.1.0",
    "redux": "3.5.2",
    "redux-form": "^6.1.1",
    "redux-saga": "0.11.0",
    "reselect": "2.5.3",
    "rimraf": "2.5.4",
    "sanitize.css": "4.1.0",
    "sinon": "2.0.0-pre",
    "striptags": "^2.1.1",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "validate.js": "^0.10.0",
    "warning": "3.0.0",
    "webpack": "2.1.0-beta.15",
    "webpack-hot-middleware": "2.12.2",
    "whatwg-fetch": "1.0.0"
  },
  "devDependencies": {
    "babel-preset-react-hmre": "1.1.1",
    "eventsource-polyfill": "0.9.6",
    "minimist": "1.2.0",
    "plop": "1.5.0",
    "psi": "2.0.4",
    "shelljs": "0.7.3",
    "stylelint": "7.1.0",
    "stylelint-config-standard": "12.0.0",
    "webpack-dev-middleware": "1.6.1"
  },
  "pre-commit#develop": [
    "lint:fix"
  ],
  "pre-commit#master": [
    "lint:fix"
  ]
}
